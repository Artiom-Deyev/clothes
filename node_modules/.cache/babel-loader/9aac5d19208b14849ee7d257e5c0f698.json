ript>\nexport default {\n  props: {\n    card: {\n      type: Object,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      productQuantity: 0,\n    };\n  },\n  methods: {\n    removeFromCart(card) {\n      this.$store.commit(\"REMOVE_FROM_CART\", card);\n    },\n    changeQuantity(card) {\n      let quantity = this.productQuantity;\n\n      this.$store.commit(\"INCREASE_QUANTITY\", card, quantity);\n      //console.log(quantity);\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}